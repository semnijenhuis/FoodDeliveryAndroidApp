<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fooddelivery1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.ShowClock"></activity>
        <activity android:name=".Drawing" />
        <activity android:name=".Activitys.Edit.EditMenuItem" />
        <activity android:name=".Activitys.Add.AddMenuItem" />
        <activity android:name=".Activitys.Edit.EditRestaurant" />
        <activity android:name=".Activitys.Add.AddRestaurant" />
        <activity android:name=".Activitys.RestaurantMenu" />
        <activity
            android:name=".Activitys.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>